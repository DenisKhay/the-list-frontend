{
  "name": "the-list-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "express": "4.16.4"
  },
  "scripts": {
    "analyze": "source-map-explorer --html build/static/js/main.* > ../analyze.html",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage",
    "test:ci": "CI=true react-scripts test",
    "eject": "react-scripts eject",
    "devserver": "serve -s build",
    "prodserver:dev": "nodemon server",
    "prodserver:prod": "node server"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/jest": "23.3.9",
    "@types/node": "10.12.10",
    "@types/react": "16.7.7",
    "@types/react-dom": "16.0.10",
    "husky": "1.2.0",
    "lint-staged": "8.1.0",
    "node-sass": "4.10.0",
    "nodemon": "1.18.6",
    "prettier": "1.15.3",
    "react": "16.6.3",
    "react-dom": "16.6.3",
    "react-scripts": "2.1.1",
    "source-map-explorer": "1.6.0",
    "typescript": "3.1.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test:ci && npm run build"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
